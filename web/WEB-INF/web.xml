<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>InsererActivite</servlet-name>
        <servlet-class>servlet.InsererActivite</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsererBouquet</servlet-name>
        <servlet-class>servlet.InsererBouquet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertBouquetActivite</servlet-name>
        <servlet-class>servlet.InsertBouquetActivite</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListeBouquetActivite</servlet-name>
        <servlet-class>servlet.ListeBouquetActivite</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestLogin</servlet-name>
        <servlet-class>servlet.TestLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsererCategorieLieu</servlet-name>
        <servlet-class>servlet.InsererCategorieLieu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertVoyage</servlet-name>
        <servlet-class>servlet.InsertVoyage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsererActivite</servlet-name>
        <url-pattern>/InsererActivite</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsererBouquet</servlet-name>
        <url-pattern>/InsererBouquet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertBouquetActivite</servlet-name>
        <url-pattern>/InsertBouquetActivite</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListeBouquetActivite</servlet-name>
        <url-pattern>/ListeBouquetActivite</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestLogin</servlet-name>
        <url-pattern>/TestLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsererCategorieLieu</servlet-name>
        <url-pattern>/InsererCategorieLieu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertVoyage</servlet-name>
        <url-pattern>/InsertVoyage</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
